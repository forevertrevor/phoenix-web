<?xml version="1.0" encoding="UTF-8" ?>

<taglib version="2.0">
    
    <description>Phoenix User Agent Switch Tag</description>
    <display-name>Phoenix User Agent</display-name>
    <tlib-version>1.0</tlib-version>
    <short-name>ua</short-name>

	<tag>
		<name>switch</name>
		<tagclass>com.mws.phoenix.web.tag.UserAgentSwitchTag</tagclass>
		<bodycontent>scriptless</bodycontent>
		<attribute>
			<name>names</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>markers</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>default</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>case</name>
		<tagclass>com.mws.phoenix.web.tag.UserAgentCaseTag</tagclass>
		<bodycontent>scriptless</bodycontent>
		<attribute>
			<name>agent</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>