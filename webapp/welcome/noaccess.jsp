<%@ taglib uri="http://jakarta.apache.org/struts/tags-html" prefix="html" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %>
<html:html>
<head>
    <title><fmt:message key="noaccess.title"/></title>
	<link rel="stylesheet" type="text/css" href='<html:rewrite href="/action/style?property=css"/>' />
</head>
<body>
	<div class="internal display plain">
	<h1><fmt:message key="noaccess.title"/></h1>
	<html:messages id="msg" header="errors.header" footer="errors.footer">
		<b class="error">${msg}</b>
	</html:messages>
    <p><fmt:message key="noaccess.desc"/></p>
    </div>
</body>
</html:html>